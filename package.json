{
  "name": "musalup",
  "module": "index.js",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development bun --hot ./src/index.js",
    "build": "bun build --production ./src/index.js --outdir=./dist --target=bun --public-path / --sourcemap --minify --env='PUBLIC_*'",
    "start": "NODE_ENV=production bun ./src/index.js",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "type": "module",
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-router": "^7.8.1"
  }
}
